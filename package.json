    "typeorm-typedi-extensions": "^0.2.3"
  },
  "scripts": {
    "dev": "cd src &&  NODE_ENV=development nodemon --exec ts-node app.ts -e ts,graphql",
    "start": "cd src &&  NODE_ENV=production nodemon --exec ts-node app.ts -e ts,graphql",
    "lint": "tslint ./src/**/*.ts --fix",
    "test:feature": "jest -c jest.config.feature.js --passWithNoTests --forceExit --detectOpenHandles",
    "test:unit": "jest -c jest.config.unit.js --passWithNoTests --forceExit --detectOpenHandles",
    "entity:create": "ts-node ./node_modules/typeorm/cli.js entity:create -n",
    "migrate": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrate:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "migration:show": "ts-node ./node_modules/typeorm/cli.js migration:show",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "tslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test:feature"
    }
  }
}
